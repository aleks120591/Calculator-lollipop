<resources>
    <declare-styleable name="EqualsImageButton">
        <attr name="state_equals" format="boolean" />
        <attr name="state_next" format="boolean" />
    </declare-styleable>

    <declare-styleable name="BackspaceImageButton">
        <attr name="state_delete" format="boolean" />
        <attr name="state_clear" format="boolean" />
    </declare-styleable>

    <declare-styleable name="GraphView">
        <attr name="showGrid" format="boolean" />
        <attr name="showInlineNumbers" format="boolean" />
        <attr name="showOutline" format="boolean" />
        <attr name="panEnabled" format="boolean" />
        <attr name="zoomEnabled" format="boolean" />
        <attr name="backgroundColor" format="color|reference" />
        <attr name="gridColor" format="color|reference" />
        <attr name="graphColor" format="color|reference" />
        <attr name="numberTextColor" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="SolidLayout">
        <attr name="preventParentTouchEvents" format="boolean" />
        <attr name="preventChildTouchEvents" format="boolean" />
    </declare-styleable>
</resources>
